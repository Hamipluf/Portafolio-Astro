---
import Layout from "@/layouts/Layout.astro";
import Hero from "@/components/Hero";
const { currentLocale } = Astro;
---

<Layout title="Home">
  <section id="Hero" class="relative mx-2 left-10 lg:left-20 overflow-hidden min-h-screen">
    <Hero currentLocale={currentLocale} client:idle />
  </section>
</Layout>
